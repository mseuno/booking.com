package com.epam.lab.model.helper;

import com.epam.lab.dao.annotations.Column;
import com.epam.lab.dao.annotations.Id;
import com.epam.lab.dao.annotations.Table;

@Table(name = "hotel_service")
public class HotelesServices {

	@Id
	@Column(name = "hotel_service.id")
	private Long id;

	@Column(name = "hotel_service.hotel_id")
	private Long hotelId;

	@Column(name = "hotel_service.service_id")
	private Long serviceId;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getHotelId() {
		return hotelId;
	}

	public void setHotelId(Long hotelId) {
		this.hotelId = hotelId;
	}

	public Long getServiceId() {
		return serviceId;
	}

	public void setServiceId(Long serviceId) {
		this.serviceId = serviceId;
	}

}
